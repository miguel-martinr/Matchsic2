{
  "name": "matchsic",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build && npm run backend/compile",
    "serve": "vite preview",
    "start": "node backend/dist/index.js",
    "backend/watch": "cd backend && tsc-watch --onSuccess \"node dist/index.js\"",
    "backend/start": "cd backend && node dist/index.js",
    "backend/compile": "cd backend && tsc",
    "backend/compile-and-start": "cd backend && tsc && node dist/index.js",
    "backend/test": "cypress run --spec \"cypress/integration/backend/**/*.spec.ts\"",
    "fe-services/test": "cypress run --spec \"cypress/integration/src/features/services/**/*.spec.ts\"",
    "cy/backend": "start-server-and-test backend/compile-and-start http://localhost:3000 backend/test",
    "cy/fe-services": "start-server-and-test backend/start http://localhost:3000 fe-services/test",
    "cypress:open": "cypress open",
    "coverall": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "engines": {
    "node": ">=16.13.0 <17"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.2",
    "@types/cookie-parser": "^1.4.2",
    "axios": "^0.24.0",
    "bootstrap": "^5.0.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "leaflet": "^1.7.1",
    "mongoose": "^6.0.14",
    "react": "^17.0.0",
    "react-bootstrap": "^2.0.3",
    "react-bootstrap-navbar-offcanvas": "^1.0.3",
    "react-dom": "^17.0.0",
    "react-leaflet": "^3.2.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.0.2",
    "react-select": "^5.2.2",
    "spotify-web-api-ts": "^1.4.5"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.12",
    "@types/cors": "^2.8.12",
    "@types/cypress": "^1.1.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/leaflet": "^1.7.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/sinon": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "cors": "^2.8.5",
    "coveralls": "^3.1.1",
    "cypress": "^9.1.0",
    "cypress-mongodb": "^2.1.2",
    "eslint": "^8.3.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-react": "^7.27.1",
    "jest": "^27.4.7",
    "start-server-and-test": "^1.14.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.9.2",
